{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "show-manager/fancy-header",
    "title": "Fancy Header",
    "category": "text",
    "description": "adds a header with a hover effect",
    "keywords": ["header", "underline", "hover"],
    "version": "1",
    "textdomain": "show-manager",
    "editorScript": "file:./index.js",
    "attributes": {
        "content": {
          "type": "string",
          "source": "html",
          "selector": "h2"
        },
        "underline_color": {
          "type": "string",
          "default": "#f87171"
        }
    },
    "style": "file:./index.css"
}